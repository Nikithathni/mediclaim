#!groovy


node{
   
    stage('CheckOutCode'){
        git branch: 'master', url: 'https://github.com/Nikithathni/mediclaim.git'
    }
   
    stage('Build'){
      if(isUnix()){
          sh 'mvn clean package'
      }else{
          bat 'mvn clean package'
      }
    }
   
    stage('SonarQubeReport'){
      if(isUnix()){
          sh 'mvn  sonar:sonar'
      }else{
          bat 'mvn sonar:sonar'
      }
    }
   
     stage('UploadintoNexus'){
      if(isUnix()){
          sh 'mvn  deploy'
      }else{
          bat 'mvn deploy'
      }
    }
}
